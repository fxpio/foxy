{
    "name": "foxy/foxy",
    "description": "Fast, reliable, and secure NPM/Yarn/pnpm bridge for Composer",
    "keywords": ["npm", "yarn", "composer", "bridge", "dependency manager", "package", "asset", "nodejs"],
    "homepage": "https://github.com/fxpio/foxy",
    "type": "composer-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "FranÃ§ois Pluchino",
            "email": "francois.pluchino@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.3",
        "composer-plugin-api": "^1.1|^2.0"
    },
    "require-dev": {
        "composer/composer": "^2.0.0",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Foxy\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra": {
        "class": "Foxy\\Foxy",
        "branch-alias": {
            "dev-main": "1.2-dev"
        }
    }
}
